  "bugs": {
    "url": "https://github.com/Jackie1210/algorithms-repo/issues"
  },
  "homepage": "https://github.com/Jackie1210/algorithms-repo#readme",
  "devDependencies": {
    "chalk": "4.1.2",
    "lint-staged": "12.0.3",
    "prettier": "2.4.1",
    "ts-node": "9.1.1",
    "typescript": "4.5.2",
    "yorkie": "2.0.0"
  }
}
