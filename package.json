{
  "name": "algorithms-repo",
  "version": "1.0.0",
  "description": "This repo is just for relearning algorithms and DS.",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jackie1210/algorithms-repo.git"
